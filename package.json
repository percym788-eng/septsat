{
  "name": "septsat-mac-auth",
  "version": "1.0.0",
  "description": "MAC Address Authentication System with Custom Database",
  "main": "auth-mac.js",
  "scripts": {
    "start": "node auth-mac.js",
    "admin": "node mac-admin.js",
    "test": "node -e \"const auth = require('./auth-mac'); new auth().quickAuth();\"",
    "backup": "node -e \"const auth = require('./auth-mac'); new auth().backupDatabase().then(r => console.log('Backup:', r));\"",
    "stats": "node -e \"const auth = require('./auth-mac'); new auth().getAuthStats().then(r => console.log(JSON.stringify(r.stats, null, 2)));\""
  },
  "bin": {
    "mac-admin": "./mac-admin.js",
    "mac-auth": "./auth-mac.js"
  },
  "keywords": [
    "mac-address",
    "authentication",
    "security",
    "database",
    "authorization"
  ],
  "author": "percym788-eng",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "directories": {
    "lib": "./",
    "test": "./tests",
    "doc": "./docs"
  },
  "files": [
    "auth-mac.js",
    "macdb.js",
    "mac-admin.js",
    "examples/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/percym788-eng/septsat.git"
  },
  "bugs": {
    "url": "https://github.com/percym788-eng/septsat/issues"
  },
  "homepage": "https://github.com/percym788-eng/septsat#readme"
}
